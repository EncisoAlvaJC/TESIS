require('psd')
set.seed(2018)
le1 = 100000
le2 = 1000
ee = rnorm(le2 + 2*le1 + 1)
tt = (1:le1)
gg = sin(tt)/tt-sin(2*tt)/tt
#gg = sin(tt)/tt #- sin(tt*.5)/tt
gg = c(rev(gg),0,gg)
le1 = length(gg)
pn = rep(0,le2)
for(i in 1:le2){
pn[i] = sum(gg*ee[1:le1+ i-1])
}
s = pspectrum(pn,Nyquist.normalize = F)
plot(pn,type='l')
plot(s)
